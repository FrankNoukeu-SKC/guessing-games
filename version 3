# guessing-games

import random

number = random.randint(1, 10)
print(number)

guess = int(input("Enter a number between 1 and 10: "))

if guess == number:
    print("Correct!")
    print("Well done!")
elif guess < number:
    print("Hard luck")
    print("Too low")
else:
    print("Hard luck!")
    print("Too high!")
    
print("Goodbye")
