# guessing-games

import random

number = random.randint(1, 10)
print(number)

correct = False

while not correct:
    
    guess = int(input("Enter a number between 1 and 10: "))
    if guess == number:
        print("Correct")
        correct = True
    elif guess < number:
        print("Too low")
    else:
        print("Too high")
        
print("Goodbye")
